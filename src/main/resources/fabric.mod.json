{
  "schemaVersion": 1,
  "id": "neuracraft",
  "version": "${version}",
  "name": "NeuraCraft",
  "description": "",
  "authors": [],
  "contact": {},
  "license": "Apache-2.0",
  "icon": "assets/neuracraft/icon.png",
